        "def solution(x):\n",
        "    arr = list(str(x))\n",
        "    sum_ = 0\n",
        "    for i in range (len(arr)) :\n",
        "        sum_ += int(arr[i])\n",
        "    if x%sum_ ==0:\n",
        "        answer = True\n",
        "    else:\n",
        "        answer = False\n",
        "    return answer"
